{
  "name": "reactbootstrap",
  "version": "1.0.0",
  "description": "react bootstrap practice",
  "main": "index.js",
  "scripts": {
    "start": "./node_modules/.bin/webpack-dev-server --config=./webpack.config.js --progress --colors",
    "build": "./node_modules/.bin/better-npm-run build",
    "build:watch": "./node_modules/.bin/better-npm-run build:watch",
    "build:prod": "./node_modules/.bin/better-npm-run build:prod"
  },
  "betterScripts": {
    "build": {
      "command": "./node_modules/.bin/webpack --progress --colors --display-error-details --display-modules",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:watch": {
      "command": "./node_modules/.bin/webpack --watch --progress --colors --display-error-details --display-modules",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:prod": {
      "command": "./node_modules/.bin/webpack --progress --colors --display-error-details --display-modules",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "author": "dane.ding <270422767@qq.com>",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "react": "^15.3.1",
    "react-bootstrap": "^0.30.3",
    "react-dom": "^15.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "better-npm-run": "0.0.11",
    "clean-webpack-plugin": "^0.1.9",
    "css-loader": "^0.25.0",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^1.15.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.2.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.21.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.2"
  }
}
